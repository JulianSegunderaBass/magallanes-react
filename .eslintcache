ster', cursive;\r\n        font-weight: lighter;\r\n        letter-spacing: 1px;\r\n    }\r\n    a {\r\n        color: ${linkColor};\r\n        text-decoration: none;\r\n        transition: color ease 0.3s;\r\n        &:hover {\r\n            color: ${hoverColor};\r\n        }\r\n    }\r\n    ul {\r\n        display: flex;\r\n        list-style: none;\r\n    }\r\n    li {\r\n       padding-left: 2rem;\r\n       /* For line animations */\r\n       position: relative;\r\n    }\r\n    @media (max-width: 870px) {\r\n        flex-direction: column;\r\n        ul {\r\n            padding-top: 1rem;\r\n            justify-content: space-around;\r\n            align-items: center;\r\n            width: 100%;\r\n            li {\r\n                padding: 0.5rem 0rem;\r\n            }\r\n        }\r\n    }\r\n    @media (max-width: 660px) {\r\n        padding: 1rem 1rem;\r\n        #logo {\r\n            /* Inline block is needed so padding/margin\r\n            can be applied to the a tag, which by default is inline */\r\n            display: inline-block;\r\n            margin: 1rem;\r\n        }\r\n        ul {\r\n            /* Column so nav elements are stacked vertically */\r\n            flex-direction: column;\r\n            padding: 0.5rem;\r\n            justify-content: space-around;\r\n            align-items: center;\r\n            width: 100%;\r\n            li {\r\n                padding: 0.5rem 0rem;\r\n            }\r\n        }\r\n    }\r\n`\r\n\r\n\r\nexport default Nav;","C:\\Users\\Julian Bass\\Documents\\WebDev Exercises\\react\\magallanes-react\\src\\assets\\Animations.js",[],"C:\\Users\\Julian Bass\\Documents\\WebDev Exercises\\react\\magallanes-react\\src\\assets\\AutoScroll.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":10,"column":10,"nodeType":"76","messageId":"77","endLine":10,"endColumn":16},{"ruleId":"74","severity":1,"message":"75","line":9,"column":10,"nodeType":"